      SluzbenikService.getZahtevi().then(\r\n            (_zahtevi) => {\r\n                let XMLParser = require('react-xml-parser');\r\n\r\n                let xml = new XMLParser().parseFromString(_zahtevi);\r\n\r\n                this.setState({zahtevi: _zahtevi});\r\n            }\r\n        )\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <PortalContext.Consumer>\r\n                { context => (\r\n                    <div className=\"main-container container\">\r\n                        <h1>Sluzbenik</h1>\r\n                        <Link to=\"/safe/addObavestenje\" >\r\n                            <Button>Dodaj obavestenje</Button>\r\n                        </Link>\r\n                        <Button className=\"mt-5\" onClick={this.getZahtevi}>Daj sve</Button>\r\n                        {this.state.zahtevi}\r\n                    </div>\r\n                )}\r\n            </PortalContext.Consumer>\r\n        )\r\n    }\r\n};\r\n\r\nexport default Sluzbenik;","D:\\Users\\Hp Zbook 15\\Desktop\\xml-projekat-2020\\Frontend\\app-portal-organ-vlasti\\src\\components\\Sluzbenik\\Obavestenje\\AddObavestenje.js",[],"D:\\Users\\Hp Zbook 15\\Desktop\\xml-projekat-2020\\Frontend\\app-portal-organ-vlasti\\src\\service\\sluzbenik-service.js",[],{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","severity":1,"message":"92","line":7,"column":8,"nodeType":"93","messageId":"94","endLine":7,"endColumn":20},{"ruleId":"91","severity":1,"message":"95","line":9,"column":8,"nodeType":"93","messageId":"94","endLine":9,"endColumn":17},{"ruleId":"91","severity":1,"message":"96","line":10,"column":8,"nodeType":"93","messageId":"94","endLine":10,"endColumn":22},{"ruleId":"91","severity":1,"message":"97","line":3,"column":12,"nodeType":"93","messageId":"94","endLine":3,"endColumn":18},{"ruleId":"91","severity":1,"message":"98","line":2,"column":8,"nodeType":"93","messageId":"94","endLine":2,"endColumn":17},{"ruleId":"91","severity":